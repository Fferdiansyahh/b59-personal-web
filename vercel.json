{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    },
    {
      "src": "Asset/**",
      "use": "@vercel/static"
    },
    {
      "cleanUrls": true
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
}
